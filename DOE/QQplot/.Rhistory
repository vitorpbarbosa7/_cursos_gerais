eval_f <- function(x){
f1 = 2629
f1a = 4418
f2 = 520
f3 = 6670
obj = (f1 - x[1])**2 + (f1a - x[2])**2 + (f2 - x[3])**2 + (f3 - x[4])**2
return(obj)
}
